page {
  padding: 20rpx;
}
.search_row {
  height: 80rpx;
  display: flex;
}
.search_row input {
  padding-left: 30rpx;
  border-radius: 15rpx;
  background-color: #d3d0d0;
  flex: 7;
  height: 100%;
}
.search_row button {
  flex: 2;
  width: 110rpx;
  height: 100%;
  display: flex;
  padding: 0;
  margin: 0 10rpx;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  background-color: #d3d0d0;
}
.search_content {
  margin-top: 30rpx;
}
.search_content .search_item {
  background-color: #fff;
  font-size: 26rpx;
  padding: 15rpx 10rpx;
  border-bottom: 1rpx solid #e7e4e4;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
