.pupContentBG {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
}
.pupContent {
  width: 100%;
  background: #f3efef;
  position: absolute;
  bottom: 0;
  box-shadow: 0 0 10rpx #333;
  border-top-left-radius: 2%;
  border-top-right-radius: 2%;
  height: 0;
  z-index: 999;
}
.pupContent .resume_head {
  width: 90%;
  margin: 30rpx auto;
}
.pupContent .resume_wrap {
  margin-bottom: 80rpx;
}
.pupContent .resume_wrap .raw {
  width: 90%;
  margin: 0 auto;
  padding: 10rpx;
  background-color: rgba(223, 222, 222, 0.4);
  border-radius: 20rpx;
}
.pupContent .tool_item {
  flex: 1px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  position: relative;
}
.pupContent .tool_item button {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
.pupContent .btn_buy {
  flex: 4;
  background-color: var(--themeColor);
  border: 1rpx var(--themeColor) solid;
  border-radius: 20rpx;
  color: #fff;
  font-size: 40rpx;
  padding: 20rpx;
  font-weight: 500;
}
.pupContent #isSubmit {
  color: var(--themeColor);
  background-color: #fff;
}
/* 设置显示的背景 */
.showBG {
  display: block;
}
.hideBG {
  display: none;
}
/* 弹出或关闭动画来动态设置内容高度 */
@keyframes slideBGtUp {
  from {
    background: transparent;
  }
  to {
    background: rgba(0, 0, 0, 0.3);
  }
}
@keyframes slideBGDown {
  from {
    background: rgba(0, 0, 0, 0.3);
  }
  to {
    background: transparent;
  }
}
/* 显示或关闭内容时动画 */
.openBG {
  animation: slideBGtUp 0.1s ease-in both;
  /* animation-fill-mode: both 动画将会执行 forwards 和 backwards 执行的动作。 */
}
.closeBG {
  animation: slideBGDown 0.1s ease-in both;
  /* animation-fill-mode: both 动画将会执行 forwards 和 backwards 执行的动作。 */
}
/* 设置显示内容 */
.showContent {
  display: block;
}
.hideContent {
  display: none;
}
/* 弹出或关闭动画来动态设置内容高度 */
@keyframes slideContentUp {
  from {
    height: 0;
  }
  to {
    height: 800rpx;
  }
}
@keyframes slideContentDown {
  from {
    height: 800rpx;
  }
  to {
    height: 0;
  }
}
/* 显示或关闭内容时动画 */
.open {
  animation: slideContentUp 0.2s ease-in both;
  /* animation-fill-mode: both 动画将会执行 forwards 和 backwards 执行的动作。 */
}
.close {
  animation: slideContentDown 0.2s ease-in both;
  /* animation-fill-mode: both 动画将会执行 forwards 和 backwards 执行的动作。 */
}
